<script type="text/javascript">
$(document).ready(function() {
show_user();
});
function show_user(){
var is_stage = '';
var is_user  = '1';
var is_alert = '1';
var is_sp    = '';
var nickname = 'Koba';
var user_img = 'https://cdn.findfriends.jp/img/1479/14795679/avatar_1_0_s.gif?986';
if(is_sp) {
$('.sp_nk_btn').show();
}
if(! is_stage) {
$('.header_stage_area').remove();
}
if(is_user){
//ログイン中
$('.login_show').show();
if(is_alert) {
$('.Alert_New').show();
}
} else {
//未ログイン
$('.no_login_show').show();
$('.login_show').remove();
}
}
</script>
