<script>
function showTopTV( _show_id )
{
var _data = {};
_data.input    = 'UTF-8';
_data.output   = 'jsonp';
_data.show_id  = _show_id;
jQuery.ajax( { type : 'GET',
url      : 'https://tv.netkeiba.com/' + 'api/api_get_top_tv_pc.html',
data     : _data,
dataType : _data.output,
success  : function( data )
{
if(data.status == 'NG'){
console.log('status:NG reason:'+ data.reason);
}else{
jQuery( '#' + _show_id ).html( data );
}
}
} );
}
</script>
