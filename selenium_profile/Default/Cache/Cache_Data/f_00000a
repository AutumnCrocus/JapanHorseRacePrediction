<script>
var _contents_action_api_url = "https://news.netkeiba.com/";
$( document ).ready( function() {
if ("IntersectionObserver" in window) {
var target = document.querySelector('#common_user_rank_1');
var option = {
root: null,
rootMargin: "0px",
threshold: [0]
};
var callback = function (entries, observer) {
entries.forEach(function (entry) {
if (entry.isIntersecting) {
showCommonUserRank( 'common_user_rank_1', 1, 5, 1, 'https://news.netkeiba.com/?pid=commentator_ranking' );
setTimeout(function(){
var data = $( '#common_user_rank_1').html();
$( '#common_user_rank_2').html( data );
},1000);
observer.unobserve(entry.target);
}
});
};
var observer = new IntersectionObserver(callback, option);
observer.observe(target);
} else {
showCommonUserRank( 'common_user_rank_1', 1, 5, 1, 'https://news.netkeiba.com/?pid=commentator_ranking' );
setTimeout(function(){
var data = $( '#common_user_rank_1').html();
$( '#common_user_rank_2').html( data );
},1000);
}
$('.Commentator_tab li').click(function() {
$('.Commentator_tab li').removeClass('Active');
$(this).addClass('Active');
var rank_type = $(this).data('sort')
var len = $('#common_user_rank_2 #api_common_rank_2').length;
if (!len) {
showCommonUserRank( 'common_user_rank_2', rank_type, 5, 1, 'https://news.netkeiba.com/?pid=commentator_ranking' );
}
$('.common_user_rank').hide();
$('#common_user_rank_'+ rank_type).show();
});
} );
</script>
</section>
